<launch>
  <arg name="rosbridge_address" default="192.168.43.15" />
  <arg name="rosbridge_port" default="9090" />

  <!-- run ros bridge -->
  <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch">
    <arg name="port" value="$(arg rosbridge_port)"/>
    <arg name="address" value="$(arg rosbridge_address)"/>
  </include>

  <!-- video web server -->
  <node name="web_video_server" pkg="web_video_server" type="web_video_server" output="screen">
    <param name="port" value="6563"/>
  </node>

  <!-- start web server -->
  <node name="http_server" pkg="web_viewer" type="server.py" output="screen" />
</launch>